{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "commonjs": true,
    "node": true,
    "browser": true,
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "function-paren-newline": 0,
    "implicit-arrow-linebreak": 0,
    "no-debugger": 0,
    "one-var-declaration-per-line": 0,
    "operator-linebreak": 0,
    "object-shorthand": 0,
    "no-confusing-arrow": 0,
    "import/prefer-default-export": 0,
    "global-require": 0,
    "no-console": 0,
    "no-return-await": 0,
    "max-len": [
      1,
      120,
      2,
      {
        "ignoreComments": true
      }
    ],
    "prop-types": [
      0
    ],
    "linebreak-style": 0,
    "indent": [
      1,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "comma-dangle": [
      1,
      "never"
    ],
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1,
    "object-curly-spacing": 1,
    "prefer-destructuring": [
      "warn",
      {
        "object": true,
        "array": false
      }
    ],
    "object-curly-newline": 0
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "airbnb-base"
  ]
}
